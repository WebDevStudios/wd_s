{
    "name": "webdevstudios/wd_s",
    "description": "A theme boilerplate for WebDevStudios.",
    "type": "wordpress-theme",
    "require-dev": {
        "webdevstudios/wds-coding-standards": "^1.1"
    },
    "scripts": {
      "sniff": "vendor/bin/phpcs --runtime-set installed_paths vendor/wp-coding-standards/wpcs,vendor/webdevstudios/wds-coding-standards/WebDevStudios -p ./ --standard=WebDevStudios --report=full --extensions=php --ignore=*/vendor/*,*/node_modules/*"
    },
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "WebDevStudios",
            "email": "contact@webdevstudios.com"
        }
    ],
    "minimum-stability": "stable"
}
